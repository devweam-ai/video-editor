{
  "name": "runway-video-creator",
  "version": "1.0.0",
  "description": "Video creation tool using Runway API",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install"
  },
  "dependencies": {
    "@google/generative-ai": "^0.17.2",
    "axios": "^1.5.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "iron-session": "^8.0.4",
    "mongoose": "^7.5.0",
    "multer": "^1.4.5-lts.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "video",
    "runway",
    "ai",
    "generation"
  ],
  "author": "Your Name",
  "license": "MIT"
}
